<h3>List of products
{{#link-to 'products.new' class='btn btn-default'}}New product{{/link-to}}
</h3>
{{outlet}}
<section>
<table class="table table-striped">
  <tr>
    <th>ID</th>
    <th>Title</th>
    <th></th>
  </tr>
  {{#each model}}
    <tr>
      <td>{{id}}</td>
      <td>{{title}}</td>
      <td>
        {{#link-to 'product.edit' this}}Edit{{/link-to}}
        <a {{action 'delete' this}} href="#">Delete</a>
      </td>
    </tr>
  {{/each}}
</table>
</section>
