<div class='col-sm-10 col-sm-offset-1 box'>
  {{#if model}}
    <h3>Closed orders</h3>
    <div class='col-sm-4'>
      <ul class="nav nav-pills nav-stacked">
        {{#each arrangedContent}}
        {{#view App.NavView}}
            {{#link-to 'orders.show' this}}
              <span class="badge pull-right">{{lineItemsCount}}</span>
              {{created_at}}
            {{/link-to}}
         {{/view}}
        {{/each}}
      </ul>
    </div>
    <div class='col-sm-8'>
      {{outlet}}
    </div>
  {{else}}
    <h3>There are no closed orders to show</h3>
  {{/if}}
</div>
